<div class="row">
  <div class="large-10 large-offset-1 columns">
  <h1>All Magazines</h1>
  <p><%= link_to 'Add a New Magazine', new_magazine_path %></p>
  <table>
     <thead>
      <tr>
        <th width="200">Magazine</th>
        <th width="150">Alternate Titles</th>
        <th width="150">Edit</th>
        <th width="150">Delete</th>
      </tr>
    </thead>
    <tbody>
      <% @magazines.each do |magazine| %>
      <tr>
        <td><%= magazine.title %></td>
        <td><%= magazine.alternate_titles %></td>
        <td><%= link_to 'Edit', edit_admin_magazine_path(magazine) %></td>
        <td><%= link_to 'Delete', admin_magazine_path(magazine), method: :delete %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>

